pipeline {
    environment {
        docker_registry = 'sounakpatra/jenkins_test'
        docker_credential = 'github'
        dockerImage = ''
    }
    agent any
    stages {
        stage('Building Image') {
            steps {
                script {
                    dockerImage = docker.build registry + ':$BUILD_NUMBER'
                }					
            }
        }
    }
}