pipeline {
    agent {
        dockerfile true
    }
    stages {
        stage('Build') {
            steps {
                sh '''
                pwd
                ls -la
                python3 -V
                touch /home/jenkins/my_app/test.py
                cat /etc/os-release
                ls -la /home/jenkins/my_app/
                cd /home/jenkins/my_app/
                python3 set_up.py
                '''
            }
        }
    }
}